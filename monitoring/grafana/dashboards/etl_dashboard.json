{
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Total Rows Processed",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(etl_processed_records_total{status=\"total\"}) by (processor_type)",
          "legendFormat": "{{processor_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 0
      }
    },
    {
      "title": "Validation Errors",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(etl_errors_total{error_type=\"validation\"}) by (processor_type)",
          "legendFormat": "{{processor_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 0
      }
    },
    {
      "title": "Processing Errors",
      "type": "stat",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(etl_errors_total{error_type=\"processing\"}) by (processor_type)",
          "legendFormat": "{{processor_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 0
      }
    },
    {
      "title": "Success Rate",
      "type": "gauge",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(etl_processed_records_total{status=\"success\"}) by (processor_type) / sum(etl_processed_records_total{status=\"total\"}) by (processor_type) * 100",
          "legendFormat": "{{processor_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "unit": "percent"
        }
      }
    },
    {
      "title": "Error Distribution",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(etl_errors_total) by (processor_type, error_type)",
          "legendFormat": "{{processor_type}} - {{error_type}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "title": "ETL Error Logs",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "loki"
      },
      "targets": [
        {
          "expr": "{job=\"etl_logs\"}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "options": {
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true,
        "prettifyLogMessage": true,
        "enableLogDetails": true
      }
    }
  ],
  "refresh": "5s",
  "schemaVersion": 38,
  "style": "dark",
  "tags": ["etl"],
  "templating": {
